<!DOCTYPE html>
<html >
<head>
  <meta charset="UTF-8">
  <title>Server Monitoring</title>


  <link rel='stylesheet prefetch' href='http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css'>
  <link rel="stylesheet" href="stylesheets/style.css">

  <script src='http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script>
  <script src='/socket.io/socket.io.js'></script>

  <script>
    $(document).ready(function(){

      var socket = io.connect();
      socket.emit('join', 'stat_info');

      socket.emit('list','stat_info');
      socket.on('message', function (message) {
        alert(JSON.stringify(message));
      })
    });

  </script>

</head>

<body>
  <div id="wrapper" >
  <!-- Sidebar -->
  <div id="sidebar-wrapper">
    <% include ./menu.ejs%>
  </div>
  <!-- Page Content -->
  <div class="page-content-wrapper">
    <div class="container-fluid">
      <div class="row">
        <div class="col-md-12">
          <a href="#" class="btn btn-primary" id="menu-toggle">Menu</a>
          <!-- top  -->
          <% include ./top.ejs %>
          <p>System Information</p>
          <!--  -->
        </div>

        <!-- middle -->
        <!--  -->
      </div>

      <div class="container">
        <table class="table table-fixed">
          <thead>
            <tr>
              <th class="col-xs-3">항목</th>
              <th class="col-xs-3">내용</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td class="col-xs-3">CPU Model</td>
              <td class="col-xs-3">data</td>
            </tr>

            <tr>
              <td class="col-xs-3">Frequency</td>
              <td class="col-xs-3">data</td>
            </tr>
            <tr>
              <td class="col-xs-3">Cache size</td>
              <td class="col-xs-3">data</td>
            </tr>
            <tr>
              <td class="col-xs-3">stepping</td>
              <td class="col-xs-3">data</td>
            </tr>
            <tr>
              <td class="col-xs-3">Vendor ID</td>
              <td class="col-xs-3">data</td>
            </tr>
            <tr>
              <td class="col-xs-3">Bogomips</td>
              <td class="col-xs-3">data</td>
            </tr>
            <tr>
              <td class="col-xs-3">Memory (total)</td>
              <td class="col-xs-3">data</td>
            </tr>
            <tr>
              <td class="col-xs-3">Memory (free)</td>
              <td class="col-xs-3">data</td>
            </tr>
            <tr>
              <td class="col-xs-3">Memory (swap)</td>
              <td class="col-xs-3">data</td>
            </tr>
            <tr>
              <td class="col-xs-3">OS</td>
              <td class="col-xs-3"></td>
            </tr>
            <tr>
              <td class="col-xs-3">Kernel</td>
              <td class="col-xs-3">data</td>
            </tr>
            <tr>
              <td class="col-xs-3">Hostname</td>
              <td class="col-xs-3">data</td>
            </tr>
            <tr>
              <td class="col-xs-3">IP</td>
              <td class="col-xs-3">data</td>
            </tr>
            <tr>
              <td class="col-xs-3">Disk</td>
              <td class="col-xs-3"></td>
            </tr>
            <tr>
              <td class="col-xs-3">Disk</td>
              <td class="col-xs-3"></td>
            </tr>
            <tr>
              <td class="col-xs-3">Disk</td>
              <td class="col-xs-3">usage_disk</td>
            </tr>
            <tr>
              <td class="col-xs-3">Filesystem</td>
              <td class="col-xs-3">stat_disk</td>
            </tr>
          </tbody>
        </table>
      </div>
      <!-- bottom -->
      <!--  -->
    </div>
  </div>

</div>
  <script src='http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js'></script>

  <script src="javascripts/index.js"></script>

</body>
</html>
